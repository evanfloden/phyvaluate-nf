manifest {
    mainScript = 'phyvaluate.nf'
}

profiles {
    crg {
        process {
          executor='crg'
          queue='cn-el7'
          memory="20G"
          cpus=2
          scratch=true
       }
       
       process.$alignUPP.container='cbcrg/pasta_upp'
       process.$alignUPP.queue="cn-el7"
       process.$alignUPP.memory="40G"
       process.$alignUPP.cpus=1
       process.$alignUPP.scratch=true      
   
       docker { enabled = true }

   }
}



env {
    PERL5LIB="$PERL5LIB:$baseDir/perl5lib"
}
